cli <- climate_change
#divisi贸n de base de datos en train/test
train <- subset(cli,Year<=2006)
test <- subset(cli,Year>2006)
#regresi贸n lineal con todas las variables
clireg <- lm(Temp~.,data = train)
summary(clireg)
#correlaci贸n entre todas las variables
cor(train)
#N2O tiene una alta correlaci贸n con CO2, CH4, CFC.12, se reduce el modelo sin esas variables
Nreg <- lm(Temp~MEI+TSI+Aerosols+N2O,data = train)
summary(Nreg)
#optimizacion del modelo inicial clireg
cli <- lm(Temp~MEI + CO2 + CH4 + N2O + CFC.11 + CFC.12 + TSI + Aerosols, data=train)
summary(cli)
cleRed <- step(cli)
summary(cleRed)
pred <- predict(cleRed,newdata = test)
summary(pred)
SSE <- sum((pred-test$Temp)^2)
SST <- sum((mean(train$Temp)-test$Temp)^2)
R2 <- 1-SSE/SST
R2
